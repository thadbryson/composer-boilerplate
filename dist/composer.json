{
    "name":         "thadbryson/{ Project name here. }",
    "description":  "{ Project description. }",
    "type":         "library",
    "license":      "MIT",
    "authors": [
        {
            "name":     "Thad Bryson",
            "email":    "thadbry@gmail.com",
            "homepage": "http://thadbryson.co",
            "role":     "Author"
        }
    ],

    "minimum-stability": "dev",
    "prefer-stable": true,

    "repositories": [
        {
            "type": "path",
            "url":  "../my-local-project-on-my-computer"
        }
    ],

    "require": {
        "php":   ">=7.1",
        "hirak/prestissimo { A great Composer plugin. Async downloads Composer plugins. }": "@stable"
    },

    "autoload": {
        "psr-4": {
            "TCB\\-- Project code here --\\":     "src/"
        }
    },

    "require-dev": {
        "codeception/codeception": "^2.3"
    },

    "autoload-dev": {
        "psr-4": {
            "_data\\": "tests/_data/",
            "_unit\\": "tests/_unit/"
        }
    }
}
