{
    "name": "thadbryson/--name",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../toolkits/toolkit-exceptions"
        }
    ],
    "require": {
        "php": ">=7.1",
        "thadbryson/toolkit-exceptions": "*"
    },
    "autoload": {
        "psr-4": {
            "--code\\": "src/"
        },
        "files": [
            "src-functions/functions.php"
        ]
    },
    "require-dev": {
        "codeception/codeception": "^2.3"
    },
    "autoload-dev": {
        "psr-4": {
            "_data\\": "tests/_data/"
        }
    }
}
